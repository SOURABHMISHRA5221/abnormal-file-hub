build/
dist/
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
*.egg-info/
*.egg

# Package directories
node_modules/
venv/
env/
.env/

# Cache and temporary files
.cache/
.pytest_cache/
.coverage
.hypothesis/
.tox/
.temp/
.tmp/
tmp/
temp/

# IDE specific files
.idea/
.vscode/
*.swp
*.swo
.DS_Store

# Large binary files and data
*.gz
*.tar
*.zip
*.rar
*.7z
*.pdf
