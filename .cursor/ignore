*.wav
*.iso
*.dmg
*.bin
*.exe
*.dll
*.jar

# Project specific
media/uploads/
*.sqlite3
*.db
backend/data/

# Files that shouldn't be analyzed for performance reasons
saurabhmishra_*.zip

# Large generated files
*.min.js
*.min.css
*.bundle.js

# Sensitive content
.env
.env.*
secrets.json
credentials.json

# Documentation images and assets
docs/images/
